#21
n= int(input("Enter a number of rows: "))
for i in range(n):
  for j in range(0, i+1):
    print("*",end= ' ')
  print(" ")
for i in range(n+1,0,-1):
  for j in range(0,i-2):
    print("*",end= ' ')
  print(" ")
  
